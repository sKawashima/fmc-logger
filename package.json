{
  "name": "fmc-logger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -n -l -p dev:*",
    "dev:app": "next dev",
    "dev:db": "docker compose up",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "pre-install": "husky install",
    "lint-staged": "lint-staged",
    "migrate": "prisma format && prisma migrate dev && prisma generate",
    "prisma-generate": "prisma generate",
    "studio": "prisma studio",
    "ts-remove-unused": "ts-remove-unused --skip .next --skip app/"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@prisma/client": "^5.21.1",
    "@types/node": "22.8.7",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "cube-notation-normalizer": "^1.0.0",
    "cubejs": "^1.3.2",
    "dayjs": "^1.11.13",
    "dayjs": "^1.11.13",
    "eslint": "8.57.1",
    "eslint-config-next": "15.0.1",
    "evergreen-ui": "^7.1.9",
    "next": "14.2.15",
    "next-auth": "^4.24.10",
    "prisma": "^5.21.1",
    "prisma-erd-generator": "^1.11.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reset-css": "^5.0.2",
    "scrambo": "^0.3.0",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@line/ts-remove-unused": "^0.6.2",
    "@types/scrambo": "^0.3.3",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx,md}": "prettier --write"
  }
}
