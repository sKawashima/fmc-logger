{
  "name": "fmc-logger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -n -l -p dev:*",
    "dev:app": "next dev",
    "dev:db": "docker compose up",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "pre-install": "husky install",
    "lint-staged": "lint-staged",
    "migrate": "prisma format && prisma migrate dev && prisma generate",
    "prisma-generate": "prisma generate",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^5.1.0",
    "@types/node": "20.4.5",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "next-auth": "^4.22.3",
    "prisma": "^5.1.0",
    "prisma-erd-generator": "^1.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,md}": "prettier --write"
  }
}
